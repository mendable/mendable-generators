<h1>Editing <%= singular_name %></h1>

<%% form_for(<%= edit_form_path %>) do |f| %>
  <%%= f.error_messages %>

<% for attribute in attributes -%>
  <p>
    <%%= f.label :<%= attribute.name %> %><br />
    <%%= f.<%= attribute.field_type %> :<%= attribute.name %> %>
  </p>
<% end -%>
  <p>
    <%%= f.submit 'Update' %>
  </p>
<%% end %>

<%%= link_to 'Show', <%= show_path %> %> |
<%%= link_to 'Back', <%= index_path %> %>
